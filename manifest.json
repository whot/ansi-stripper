{
  "manifest_version": 2,
  "name": "ANSI Stripper",
  "description": "Removes ANSI escape sequences.",
  "homepage_url": "https://github.com/whot/ansi-stripper",
  "version": "1.0",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./emojiMap.js", "./substitute.js"]
    }
  ]
}
